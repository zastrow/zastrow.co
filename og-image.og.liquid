<style>
div {
	display: flex;
}

.root {
	background-image: url('data:image/png;base64,');
	background-color: #1e2026;
	background-size: cover;
	position: relative;
	box-sizing: border-box;
	height: 630px;
	width: 1200px;
	align-items: stretch;
	overflow: visible;
}

.content {
	overflow: visible;
	position: relative;
	padding: 20px 120px 20px 280px;
	box-sizing: border-box;
	align-items: center;
	width: 100%;
	height: 100%;
	filter:
	{%- for i in (1..10) -%}
		drop-shadow({{ i | times: 0.5 }}px {{ i | times: 0.5 }}px #0c0d0f)
	{%- endfor -%};
}

.title {
	overflow: visible;
	width: 100%;
	color: #efefef;
	font-size: {{ fontsize }};
}

.site {
	color: white;
	position: absolute;
	bottom: 30px;
	right: 30px;
	font-size: 48px;
	color: #30333d;
}

</style>

<div class="root">
	<div class="content">
		<div class="title">
			{{title}}
		</div>
	</div>
	<div class="site">zastrow.co</div>
</div>
