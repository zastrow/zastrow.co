    "@extractus/feed-extractor": "^7.0.8",
    "airtable": "^0.12.2",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "eleventy-xml-plugin": "^0.1.0",
    "fast-glob": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "open-props": "^1.6.16",
    "postcss-cli": "^10.1.0",
    "postcss-color-converter": "^1.1.0",
    "postcss-custom-media": "^10.0.2",
    "postcss-jit-props": "^1.0.14",
    "sass": "^1.69.7",
    "shelljs": "^0.8.5",
