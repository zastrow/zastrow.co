    "decap-cms-app": "^3.0.12",
    "dotenv": "^16.3.1",
    "eleventy-plugin-og-image": "^4.2.1",
    "eleventy-xml-plugin": "^0.1.0",
    "esbuild": "^0.27.2",
    "fast-glob": "^3.3.2",
