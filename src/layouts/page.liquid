---
layout-name: page
---
{% layout "default-layout" %}

{%- block main %}
	<header class="cmp-list-header">
		<h1 class="cmp-list-header__title">{{title}}</h1>
	</header>
	{%- if updated -%}
	<p><strong>Updated:</strong> {{ updated | date: "%d %B %Y" }}</p>
	{%- endif -%}
	{{ content }}
{% endblock -%}
